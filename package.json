{
  "private": true,
  "name": "ivi-todomvc",
  "version": "1.0.0",
  "scripts": {
    "build:routes": "ts-node -P ./routes/tsconfig.json ./routes/routes.config.ts > ./src/routes.ts",
    "serve": "webpack-serve --open --config webpack.dev.js",
    "dist:rollup": "tsc && rollup -c rollup.config.js",
    "dist": "webpack --config webpack.prod.js",
    "lint": "tslint --format codeFrame",
    "compress": "brotli --output=./dist/bundle.js.br ./dist/bundle.js"
  },
  "dependencies": {
    "ivi": "^0.21.0",
    "ivi-html": "^0.21.0",
    "ivi-router": "^0.21.0",
    "ivi-state": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.24",
    "cache-loader": "^2.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "ivi-tslint-rules": "^0.1.6",
    "rollup": "^1.1.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "routekit": "^0.5.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.2.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-serve": "^2.0.3"
  },
  "description": "ivi TodoMVC",
  "license": "MIT",
